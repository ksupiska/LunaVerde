<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LunaVerde</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LunaVerde.styles.css" asp-append-version="true" />
    
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=DM+Serif+Text:ital@0;1&display=swap" rel="stylesheet">
    </head>
<body>
    <header>
        <div class="logo">
            <svg width="70" height="70" viewBox="0 0 70 70" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <rect width="70" height="70" fill="url(#pattern0_25_2)" />
                <defs>
                    <pattern id="pattern0_25_2" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_25_2" transform="scale(0.0078125)" />
                    </pattern>
                    <image id="image0_25_2" width="128" height="128" 
                    xlink:href="data:image/png;base64," />
                </defs>
            </svg>
        </div>
        <nav>
            <!-- Стандартное меню -->
            <ul class="desktop-menu">
                <li><a href="#">Menu</a></li>
                <li><a href="#">About Us</a></li>
                <li><a href="#">Delivery</a></li>
            </ul>

            <!-- Иконка бургер-меню (она будет скрыта на большом экране) -->
            <div id="burger-icon" class="burger-icon">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 30 30" width="30" height="30">
                    <path d="M0 6h30v2H0zm0 7h30v2H0zm0 7h30v2H0z" fill="currentColor" />
                </svg>
            </div>

            <!-- Меню для мобильных устройств (по умолчанию скрыто) -->
            <ul id="mobile-menu" class="mobile-menu">
                <li><a href="#">Menu</a></li>
                <li><a href="#">About Us</a></li>
                <li><a href="#">Delivery</a></li>
            </ul>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - LunaVerde - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
